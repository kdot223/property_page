{
  "name": "property-page",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:next\"",
    "dev:server": "cross-env PORT=4001 node api/server.js",
    "dev:next": "next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "deploy": "git add . && git commit -m \"auto deploy\" && git push origin main"

  },
  "dependencies": {
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.13.1",
    "cors": "2.8.5",
    "cross-env": "^10.1.0",
    "express": "4.19.0",
    "next": "14.0.0",
    "react": "18.3.1",
    "react-bootstrap": "^2.10.10",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
